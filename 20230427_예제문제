/*#include <stdio.h>

int main(void) {
  int i,n,sum;
  i=1;
  sum=0;
  printf("정수를 입력하시오 : ");
  scanf("%d",&n);
  while(i<=n){
    sum=sum+i;
    i++;
  }
  printf("1부터 %d까지의 합은 %d입니다\n",n,sum);
}*/

/*#include <stdio.h>

