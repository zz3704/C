#include <stdio.h>

int main() {
  int s, e;
  int sum;
  while (1) {
    sum = s + e;
    scanf("%d %d", &s, &e);

    if ((s < 2 || e < 2) || (s > 9 || e < 2)) {

      printf("INPUT ERROR!");
      printf("값을 다시 입력하세요 : \n");
    } else {
      if (s < e) {
        for (int i = 1; i <= 9; i++) {
          for (int j = s; j <= e; j++) {
            printf("%d * %d = %2d   ", j, i, i * j);
          }
          printf("\n");
        }
      } else {

        for (int i = 1; i <= 9; i++) {
          for (int j = s; j <= e; j--) {
            printf("%d * %d = %2d   ", j, i, i * j);
          }
          printf("\n");
        }
      }
    }

    break;
  }
}
